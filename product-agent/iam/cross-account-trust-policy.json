{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Sid": "AllowCampaignAgentCrossAccountAccess",
      "Effect": "Allow",
      "Principal": {
        "AWS": "arn:aws:iam::KAMPANYA_HESAP_ID:root"
      },
      "Action": "sts:AssumeRole",
      "Condition": {
        "StringEquals": {
          "sts:ExternalId": "campaign-agent-access"
        }
      }
    }
  ]
}
